{
  "name": "zhishiq/flarum-queue",
  "description": "Say hello to the world!",
  "type": "flarum-extension",
  "license": "MIT",
  "authors": [
    {
      "name": "Bill Lu",
      "email": "lubobill1990@163.com"
    }
  ],
  "support": {
    "issues": "https://github.com/ZhiShiQ/flarum-queue/issues",
    "source": "https://github.com/ZhiShiQ/flarum-queue"
  },
  "require": {
    "flarum/core": "^0.1.0-beta.8",
    "illuminate/queue": "^5.5",
    "illuminate/redis": "^5.5"
  },
  "autoload": {
    "psr-4": {
      "ZhiShiQ\\Flarum\\Queue\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.1.x-dev"
    },
    "flarum-extension": {
      "title": "Queue",
      "icon": {
        "image": "icon.png",
        "backgroundSize": "46% 63%",
        "backgroundPosition": "center",
        "backgroundRepeat": "no-repeat",
        "backgroundColor": "#40bad8",
        "color": "#fff"
      }
    }
  }
}
